//------------------------------------------
ProcessConfig = uTimerScript                                    
{         
  AppTick    = 4
  CommsTick  = 4

  rand_var = varname=RAND_X, min=-25,  max=200, key=at_post
  rand_var = varname=RAND_Y, min=-175, max=-25, key=at_post

  event = var=VISIT_POINT, val="firstpoint", time=0.25
  event = var=VISIT_POINT, val="x=$[RAND_X], y=$[RAND_Y], id=$[TCOUNT]", time=0.25, amt=100 
  event = var=VISIT_POINT, val="lastpoint",  time=0.25
} 


